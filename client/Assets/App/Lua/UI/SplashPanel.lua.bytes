local SplashPanel = class("SplashPanel", UIBase)

function SplashPanel:OnOpen()
	self.view.cngBG.alpha = 0
	local seq = C_DOTween.Sequence()
	seq:Append(self.view.cngBG:DOFade(1, 1))
	seq:AppendInterval(1)
	seq:Append(self.view.cngBG:DOFade(0, 1))
	--动画完成回调
	seq:AppendCallback(function()
		UIMgr:CloseSelf(self)
	end)
end

function SplashPanel:OnClose()
	C_UpdateManager:CheckVersion(GameDefine.ServerAddress.SvrResIp)
end

return SplashPanel